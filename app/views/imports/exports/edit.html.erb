<%= render partial: 'imports/imports/header', locals: {import: @import} %>

<h2>
  Editing Export

  <%= link_to imports_import_export_path(@import, @export), class: 'pull-right btn btn-default' do %>
    <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
  <% end %>
  <%= link_to [@import, :exports], class: 'pull-right btn btn-default' do %>
    <span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span>
  <% end %>
</h2>
<hr/>

<%= render 'form', export: @export %>
